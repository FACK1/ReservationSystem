{
  "name": "reservationsystem",
  "version": "1.0.0",
  "description": "A web app that enables the YDRC staff to coordinate the reservation process in their organization between all of them.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "cd client && npm start",
    "start": "node start ./server/server.js && npm run client",
    "localStart": "nodemon start ./server/server.js && npm run client",
    "build": "cd client && npm run build",
    "dev": "cd client && npm run build && cd .. & npm run localStart",
    "heroku-postbuild":
      "NPM_CONFIG_PRODUCTION=false npm install --prefix client
       && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FACK1/ReservationSystem.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FACK1/ReservationSystem/issues"
  },
  "homepage": "https://github.com/FACK1/ReservationSystem#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.10"
  }
}
